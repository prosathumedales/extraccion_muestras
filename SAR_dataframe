        "#import logging\n",
        "\n",
        "#evaluo  get_rois_values_from_files(dir, shp_file)\n",
        "from datetime import datetime\n",
        "\n",
        "# Inputs\n",
        "folder = 'gdrive/MyDrive/Raster/'\n",
        "dataframes_folder = 'gdrive/MyDrive/ExtraccionMuestras_Prueba/dataframes/'\n",
        "nodatavalue=-9999\n",
        "shp_files_list = glob.glob('gdrive/MyDrive/ROIs_prueba2/Join_20220526/*shp')\n",
        "\n",
        "# Comienza el cálculo\n",
        "\n",
        "subfolders = [ f.path for f in os.scandir(folder) if f.is_dir() ]\n",
        "\n",
        "for s in subfolders:\n",
        "  tipo_de_sensor =  os.path.basename(os.path.normpath(s)).upper()\n",
        "  sensorsfolders = [ f.path for f in os.scandir(s) if f.is_dir()]\n",
        "\n",
        "  for sensor in sensorsfolders:\n",
        "    sensor_name_sat =  os.path.basename(os.path.normpath(sensor)).upper()\n",
        "    sensorandlevelfolders = [ f.path for f in os.scandir(sensor) if f.is_dir()]\n",
        "\n",
        "    df = pd.DataFrame(columns=cols) # Ojo esta linea, seguro tengamos que modificarla de acuerdo al sensor y demas, es solo para que haya un header,\n",
        "        # se puede poner despues sino\n",
        "    df.to_csv(dataframes_folder+sensor_name_sat+'_test_version.csv')\n",
        "\n",
        "    for shp_file in shp_files_list:\n",
        "      d = vector_table_to_dic(shp_file)\n",
        "\n",
        "\n",
        "      for sensor_and_level in sensorandlevelfolders:\n",
        "        if sensor_name_sat == 'SAOCOM':\n",
        "          #print(sensor_and_level)\n",
        "          for fold in [ f.path for f in os.scandir(sensor_and_level) if f.is_dir()][1:]:\n",
        "            print(fold)\n",
        "            get_csv(fold, shp_file, dataframes_folder, sensor_name_sat)\n",
        "        else:\n",
        "          get_csv(sensor_and_level, shp_file, dataframes_folder, sensor_name_sat)\n",
        "\n",
